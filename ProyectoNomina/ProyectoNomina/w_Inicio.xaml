<Window x:Class="ProyectoNomina.w_Inicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoNomina"
        mc:Ignorable="d"
        Title="w_Inicio" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Usuario" HorizontalAlignment="Left" Margin="41,72,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="132,67,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="txtPass" HorizontalAlignment="Left" Margin="132,120,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1" Content="Contraseña" HorizontalAlignment="Left" Margin="41,127,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnIniciar" Content="Iniciar Sesión" HorizontalAlignment="Left" Margin="86,183,0,0" VerticalAlignment="Top" Width="118" Click="btnIniciar_Click"/>

    </Grid>
</Window>
